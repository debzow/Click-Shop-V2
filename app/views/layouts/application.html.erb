<!DOCTYPE html>
<html>
  <head>
    <title>ClickandShop</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= favicon_link_tag asset_path('logo.png') %>

  </head>

 
  <body>
    <%= render 'renders/navbar' %>
    <%= render 'renders/deviseNotice' %>
    <%= render 'renders/btnBackToTop' %>
    <%= yield %>
    <%= render 'renders/footer' %>
  </body>
</html>
